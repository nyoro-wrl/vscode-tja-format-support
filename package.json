{
  "name": "tja-format-support",
  "displayName": "TJA Format Support",
  "description": "Taiko Jiro Format Support Extension",
  "version": "1.5.5",
  "publisher": "nyoro",
  "license": "MIT",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "keywords": [
    "tja",
    "tjc",
    "tjf",
    "tmg",
    "highlight",
    "snippets"
  ],
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "enablement": "editorLangId == tja && editorHasSelection",
        "command": "tja.zoom",
        "title": "%tja.zoom%",
        "category": "tja"
      },
      {
        "enablement": "editorLangId == tja && editorHasSelection && !editorHasMultipleSelections",
        "command": "tja.truncate",
        "title": "%tja.truncate%",
        "category": "tja"
      },
      {
        "enablement": "editorLangId == tja && editorHasSelection && !editorHasMultipleSelections",
        "command": "tja.constantScroll",
        "title": "%tja.constantScroll%",
        "category": "tja"
      },
      {
        "enablement": "editorLangId == tja && editorHasSelection && !editorHasMultipleSelections",
        "command": "tja.transitionScroll",
        "title": "%tja.transitionScroll%",
        "category": "tja"
      },
      {
        "enablement": "editorLangId == tja && editorHasSelection",
        "command": "tja.deleteCommands",
        "title": "%tja.deleteCommands%",
        "category": "tja"
      },
      {
        "enablement": "editorLangId == tja && editorHasSelection",
        "command": "tja.toBig",
        "title": "%tja.toBig%",
        "category": "tja"
      },
      {
        "enablement": "editorLangId == tja && editorHasSelection",
        "command": "tja.toSmall",
        "title": "%tja.toSmall%",
        "category": "tja"
      },
      {
        "enablement": "editorLangId == tja && editorHasSelection",
        "command": "tja.toRest",
        "title": "%tja.toRest%",
        "category": "tja"
      },
      {
        "enablement": "editorLangId == tja && editorHasSelection",
        "command": "tja.reverse",
        "title": "%tja.reverse%",
        "category": "tja"
      },
      {
        "enablement": "editorLangId == tja && editorHasSelection",
        "command": "tja.random",
        "title": "%tja.random%",
        "category": "tja"
      },
      {
        "command": "tja.changeLanguage",
        "title": "言語変更 / Change Language / 更改语言",
        "category": "tja"
      },
      {
        "command": "tja.debugLanguage",
        "title": "言語設定のデバッグ / Debug Language Settings / 调试语言设置",
        "category": "tja"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == tja && editorHasSelection",
          "command": "tja.zoom",
          "group": "0_tja_0@1"
        },
        {
          "when": "editorLangId == tja && editorHasSelection",
          "command": "tja.truncate",
          "group": "0_tja_0@2"
        },
        {
          "when": "editorLangId == tja && editorHasSelection",
          "command": "tja.toBig",
          "group": "0_tja_1@1"
        },
        {
          "when": "editorLangId == tja && editorHasSelection",
          "command": "tja.toSmall",
          "group": "0_tja_1@2"
        },
        {
          "when": "editorLangId == tja && editorHasSelection",
          "command": "tja.toRest",
          "group": "0_tja_1@3"
        },
        {
          "when": "editorLangId == tja && editorHasSelection",
          "command": "tja.reverse",
          "group": "0_tja_1@4"
        },
        {
          "when": "editorLangId == tja && editorHasSelection",
          "command": "tja.random",
          "group": "0_tja_1@5"
        },
        {
          "when": "editorLangId == tja && editorHasSelection",
          "command": "tja.constantScroll",
          "group": "0_tja_2@1"
        },
        {
          "when": "editorLangId == tja && editorHasSelection",
          "command": "tja.transitionScroll",
          "group": "0_tja_2@2"
        },
        {
          "when": "editorLangId == tja && editorHasSelection",
          "command": "tja.deleteCommands",
          "group": "0_tja_3@1"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "%config.title%",
      "properties": {
        "tjaFormatSupport.gogotimeHighlight": {
          "type": "boolean",
          "default": true,
          "description": "%config.gogotimeHighlight%"
        },
        "tjaFormatSupport.branchHighlight": {
          "type": "boolean",
          "default": true,
          "description": "%config.branchHighlight%"
        },
        "tjaFormatSupport.liteMode": {
          "type": "boolean",
          "default": false,
          "description": "%config.liteMode%"
        },
        "tjaFormatSupport.completion": {
          "type": "object",
          "description": "%config.completion%",
          "properties": {
            "tjap": {
              "type": "boolean",
              "description": "TJAPlayer"
            },
            "optk": {
              "type": "boolean",
              "description": "OpenTaiko"
            },
            "tmg": {
              "type": "boolean",
              "description": "TaikoManyGimmicks"
            }
          },
          "default": {
            "tjap": true,
            "optk": true,
            "tmg": false
          },
          "additionalProperties": false
        },
        "tjaFormatSupport.measureCountHint": {
          "type": "boolean",
          "default": true,
          "description": "%config.measureCountHint%"
        },
        "tjaFormatSupport.language": {
          "type": "string",
          "enum": [
            "auto",
            "ja",
            "en",
            "zh"
          ],
          "enumDescriptions": [
            "Auto",
            "日本語",
            "English",
            "中文"
          ],
          "default": "auto",
          "description": "%config.language%"
        }
      }
    },
    "languages": [
      {
        "id": "tja",
        "aliases": [
          "TJA",
          "tja"
        ],
        "extensions": [
          ".tja",
          ".tjc",
          ".tjf",
          ".tmg"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "tja.info",
          "name": "%view.chartInfo%",
          "when": "tja.activeFile",
          "icon": "images/icon.png"
        }
      ]
    },
    "grammars": [
      {
        "language": "tja",
        "scopeName": "source.tja",
        "path": "./syntaxes/tja.tmLanguage.json"
      }
    ],
    "semanticTokenTypes": [
      {
        "id": "roll",
        "description": "%semantic.roll%"
      },
      {
        "id": "rollBig",
        "description": "%semantic.rollBig%"
      },
      {
        "id": "balloon",
        "description": "%semantic.balloon%"
      },
      {
        "id": "balloonBig",
        "description": "%semantic.balloonBig%"
      },
      {
        "id": "fuze",
        "description": "%semantic.fuze%"
      },
      {
        "id": "gogo",
        "description": "%semantic.gogo%"
      },
      {
        "id": "gogoMeasureEnd",
        "description": "ゴーゴータイム"
      },
      {
        "id": "normal",
        "description": "普通譜面"
      },
      {
        "id": "expert",
        "description": "玄人譜面"
      },
      {
        "id": "master",
        "description": "達人譜面"
      },
      {
        "id": "header",
        "description": "ヘッダー"
      }
    ],
    "semanticTokenScopes": [
      {
        "language": "tja",
        "scopes": {
          "roll": [
            "source.tja note.roll"
          ],
          "rollBig": [
            "source.tja note.roll.big"
          ],
          "balloon": [
            "source.tja note.balloon"
          ],
          "balloonBig": [
            "source.tja note.balloon.big"
          ],
          "fuze": [
            "source.tja note.fuze"
          ],
          "gogo": [
            "source.tja gogotime note.empty"
          ],
          "gogoMeasureEnd": [
            "source.tja gogotime measureEnd"
          ],
          "normal": [
            "source.tja branch.normal note.empty"
          ],
          "expert": [
            "source.tja branch.expert note.empty"
          ],
          "master": [
            "source.tja branch.master note.empty"
          ],
          "header": [
            "source.tja header.name"
          ]
        }
      }
    ],
    "configurationDefaults": {
      "[tja]": {
        "files.encoding": "shiftjis",
        "editor.unicodeHighlight.ambiguousCharacters": false,
        "editor.snippetSuggestions": "top",
        "editor.unicodeHighlight.invisibleCharacters": false,
        "editor.suggest.showWords": false,
        "editor.occurrencesHighlight": "off",
        "editor.semanticHighlighting.enabled": true
      },
      "workbench.colorCustomizations": {
        "editorInlayHint.foreground": "#969696e7",
        "editorInlayHint.background": "#00000000"
      },
      "editor.tokenColorCustomizations": {
        "textMateRules": [
          {
            "scope": "source.tja header.name",
            "settings": {
              "foreground": "#569cd6"
            }
          },
          {
            "scope": "source.tja header.parameter",
            "settings": {
              "foreground": "#d4d4d4"
            }
          },
          {
            "scope": "source.tja header.parameter.prefix",
            "settings": {
              "foreground": "#888"
            }
          },
          {
            "scope": "source.tja command.name",
            "settings": {
              "foreground": "#cd86c7"
            }
          },
          {
            "scope": "source.tja command.delimiter",
            "settings": {
              "foreground": "#d4d4d4"
            }
          },
          {
            "scope": "source.tja command.parameter",
            "settings": {
              "foreground": "#ffc3ff"
            }
          },
          {
            "scope": "source.tja command.name.branch",
            "settings": {
              "foreground": "#ce9178"
            }
          },
          {
            "scope": "source.tja command.delimiter.branch",
            "settings": {
              "foreground": "#d4d4d4"
            }
          },
          {
            "scope": "source.tja command.parameter.branch",
            "settings": {
              "foreground": "#ffcab5"
            }
          },
          {
            "scope": "source.tja note.empty",
            "settings": {
              "foreground": "#4B4B4B"
            }
          },
          {
            "scope": "source.tja branch.normal note.empty",
            "settings": {
              "foreground": "#424b3c"
            }
          },
          {
            "scope": "source.tja branch.expert note.empty",
            "settings": {
              "foreground": "#314a5a"
            }
          },
          {
            "scope": "source.tja branch.master note.empty",
            "settings": {
              "foreground": "#50374a"
            }
          },
          {
            "scope": "source.tja gogotime note.empty",
            "settings": {
              "foreground": "#59434a"
            }
          },
          {
            "scope": "source.tja note.don",
            "settings": {
              "foreground": "#e96d46"
            }
          },
          {
            "scope": "source.tja note.don.big",
            "settings": {
              "foreground": "#e95020",
              "fontStyle": "bold"
            }
          },
          {
            "scope": "source.tja note.don.big.hand",
            "settings": {
              "foreground": "#e95020",
              "fontStyle": "bold underline"
            }
          },
          {
            "scope": "source.tja note.ka",
            "settings": {
              "foreground": "#77c4c1"
            }
          },
          {
            "scope": "source.tja note.ka.big",
            "settings": {
              "foreground": "#00acbd",
              "fontStyle": "bold"
            }
          },
          {
            "scope": "source.tja note.ka.big.hand",
            "settings": {
              "foreground": "#00acbd",
              "fontStyle": "bold underline"
            }
          },
          {
            "scope": "source.tja note.bomb",
            "settings": {
              "foreground": "#b2b2b2",
              "fontStyle": "bold"
            }
          },
          {
            "scope": "source.tja note.adlib",
            "settings": {
              "foreground": "#909090",
              "fontStyle": "underline"
            }
          },
          {
            "scope": "source.tja note.kadon.big",
            "settings": {
              "foreground": "#cd86c7",
              "fontStyle": "bold"
            }
          },
          {
            "scope": [
              "source.tja noteRange.roll note.start",
              "source.tja noteRange.roll note.empty",
              "source.tja noteRange.roll note.end"
            ],
            "settings": {
              "foreground": "#f3c02c"
            }
          },
          {
            "scope": [
              "source.tja noteRange.roll.big note.start",
              "source.tja noteRange.roll.big note.empty",
              "source.tja noteRange.roll.big note.end"
            ],
            "settings": {
              "foreground": "#f8b200",
              "fontStyle": "bold"
            }
          },
          {
            "scope": [
              "source.tja noteRange.balloon note.start",
              "source.tja noteRange.balloon note.empty",
              "source.tja noteRange.balloon note.end"
            ],
            "settings": {
              "foreground": "#e4853b"
            }
          },
          {
            "scope": [
              "source.tja noteRange.balloon.big note.start",
              "source.tja noteRange.balloon.big note.empty",
              "source.tja noteRange.balloon.big note.end"
            ],
            "settings": {
              "foreground": "#ff8400",
              "fontStyle": "bold"
            }
          },
          {
            "scope": [
              "source.tja noteRange.fuze note.start",
              "source.tja noteRange.fuze note.empty",
              "source.tja noteRange.fuze note.end"
            ],
            "settings": {
              "foreground": "#a595ff"
            }
          },
          {
            "scope": "source.tja measureEnd",
            "settings": {
              "foreground": "#d4d4d4"
            }
          }
        ]
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/color-convert": "^2.0.0",
    "@types/glob": "^8.0.0",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nyoro-wrl/vscode-tja-format-support"
  },
  "dependencies": {
    "@types/semver": "^7.5.5",
    "color-convert": "^2.0.1",
    "lodash": "^4.17.21",
    "semver": "^7.5.4"
  }
}
